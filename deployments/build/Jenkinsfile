pipeline {
  agent any
  stages {
    stage('build image') {
      }
      steps {
        withAWS(credentials)
        sh './build_web.sh GIT_BRANCH $(git tag --sort version:refname | tail -1)'
      }
    }

    stage('test image') {
      steps {
        sh 'tests would go here '
      }
    }

    stage('postbuild') {
      steps {
        echo 'send some notifications'
      }
    }

  }
}