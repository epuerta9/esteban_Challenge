pipeline {
  agent any
  stages {
    stage('build image') {
        steps {
            dir("deployments/build") {
          
                withAWS(credentials: 'epuerta-challenge', region: 'us-east-2'){
                    
                    sh' $GIT_BRANCH '
                    sh '$TAG_NAME'
                    sh './build_web.sh $GIT_BRANCH ${env.BUILD_VERSION}'
                   

                }
            }
        }
    }
  }
}
