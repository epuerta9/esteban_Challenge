pipeline {
  agent any
  stages {
    stage('build image') {
        dir('deployments/build'){
            steps {
          
                withAWS(credentials: 'epuerta-challenge', region: 'us-east-2'){
                    sh './build_web.sh GIT_BRANCH $(git tag --sort version:refname | tail -1)'
                }
            }
        }
    }
  }
}
